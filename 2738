#include <stdio.h>
int main(){
    int n1, n2, A[100][100], B[100][100];
    scanf("%d %d", &n1, &n2);
    
    for(int i = 0; i<n1; i++){
        for(int j=0; j<n2; j++){
            scanf("%d", &A[i][j]);
        }
    }

    for(int i = 0; i<n1; i++){
        for(int j=0; j<n2; j++){
            scanf("%d", &B[i][j]);
        }
    }
    
    for(int i = 0; i<n1; i++){
        for(int j=0; j<n2; j++){
            printf("%d ", A[i][j] + B[i][j]);
        }
    printf("\n");
    }
}
